{
  "name": "@humbledao/truffle",
  "version": "v1.0.32",
  "private": true,
  "scripts": {
    "postinstall": "yarn build",
    "lint:scripts": "eslint --ext \".ts,.js,.vue\" --ignore-pattern .eslintignore --ignore-pattern .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,.js --ignore-pattern .eslintignore --ignore-pattern .gitignore .",
    "lint": "yarn lint:scripts && yarn lint:style",
    "test-development": "truffle test --network development",
    "build": "truffle compile --all",
    "deploy-development": "truffle migrate --network development"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.1.7",
    "@openzeppelin/contracts": "^3.4.1",
    "@openzeppelin/contracts-upgradeable": "^3.4.1",
    "@sushiswap/sdk": "^5.0.0-canary.87",
    "eip-712": "^0.4.3",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "redirect-ssl": "^3.0.0",
    "truffle-flatten": "^1.0.8",
    "typeit": "^8.0.8",
    "unique-names-generator": "^4.6.0",
    "vec3": "^0.1.7"
  },
  "devDependencies": {
    "@openzeppelin/test-helpers": "^0.5.11",
    "@openzeppelin/truffle-upgrades": "^1.8.0",
    "@truffle/config": "^1.2.41",
    "@truffle/hdwallet-provider": "^1.5.1",
    "@truffle/provider": "^0.2.32",
    "@types/chai": "^4.3.0",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "^9.0.0",
    "abdk-libraries-solidity": "2.4.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-truffle": "^0.3.1",
    "prettier": "^2.5.1",
    "truffle": "^5.4.9",
    "truffle-contract-size": "^2.0.1",
    "truffle-plugin-verify": "^0.5.15"
  }
}
